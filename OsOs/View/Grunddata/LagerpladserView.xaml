    <Grid Background="#CFD8DC" x:Name="gridRoot">

        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.DataContext>
            <ViewModel:DataViewModel/>
        </Grid.DataContext>
        <!-- ====================================================================== -->
        <!-- ===== Pop-Up Overlay Box -->
        <!-- ====================================================================== -->
<!--        <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Grid.RowSpan="2" Canvas.ZIndex="1000" Visibility="{Binding IsVisibleOverlaySearch, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">-->
<!--                <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Grid.RowSpan="2" Canvas.ZIndex="1000">-->
                <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Grid.RowSpan="2" Canvas.ZIndex="1000" Visibility="Collapsed">

            <Rectangle VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Fill="Black" Opacity="0.7">
                <interactivity:Interaction.Behaviors>
                    <core:EventTriggerBehavior EventName="PointerPressed">
                        <core:InvokeCommandAction Command="{Binding CloseOverlaySearchCommand}"/>
                    </core:EventTriggerBehavior>
                </interactivity:Interaction.Behaviors>
            </Rectangle>
            <Grid Width="500" Height="500" VerticalAlignment="Center" HorizontalAlignment="Center" Background="#607D8B" Canvas.ZIndex="1000">
                <Grid.RowDefinitions>
                    <RowDefinition Height="24"/>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="50"/>

                </Grid.RowDefinitions>
                <AppBarButton Icon="Cancel" Foreground="#fff" Background="#F44336" HorizontalAlignment="Right" Width="80" Height="48" Margin="0 -12 -20 -12" RenderTransformOrigin="0.5,0.5">
                    <AppBarButton.RenderTransform>
                        <CompositeTransform ScaleX="-0.5" ScaleY="0.5"/>
                    </AppBarButton.RenderTransform>
                    <interactivity:Interaction.Behaviors>
                        <core:EventTriggerBehavior EventName="Click">
                            <core:InvokeCommandAction Command="{Binding CloseOverlaySearchCommand}"/>
                        </core:EventTriggerBehavior>
                    </interactivity:Interaction.Behaviors>
                </AppBarButton>
                <TextBlock VerticalAlignment="Center" Foreground="#fff" Grid.Row="1" Margin="10 0" FontSize="20" Text="{Binding OverlaySearchHeader, UpdateSourceTrigger=PropertyChanged}"/>
                <TextBox Grid.Row="2" VerticalAlignment="Center" Margin="10 0" Text="{Binding OverlaySearchTerm, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                <ListView Grid.Row="3" Margin="10" Background="#CFD8DC" SelectedItem="{Binding SelectedItemLocation, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding FilteredLocations, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="{Binding IsVisibleLocationList}" ItemTemplate="{StaticResource LocationListViewItemTemplate}" />
                <ListView Grid.Row="3" Margin="10" Background="#CFD8DC" SelectedItem="{Binding SelectedItemProduct, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding FilteredProducts, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="{Binding IsVisibleProductList}" ItemTemplate="{StaticResource ProductListViewItemTemplate}" />

                <Button Content="Ok" Grid.Row="4" Margin="10 0" Command="{Binding SetSelectedLocationCommand, UpdateSourceTrigger=PropertyChanged}" Background="#8BC34A" Foreground="#fff" Padding="15 5" Visibility="{Binding IsVisibleLocationList, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                <Button Content="Ok" Grid.Row="4" Margin="10 0" Command="{Binding SetSelectedProductCommand, UpdateSourceTrigger=PropertyChanged}" Background="#8BC34A" Foreground="#fff" Padding="15 5" Visibility="{Binding IsVisibleProductList, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                <Button Content="Annuller" Grid.Row="4" HorizontalAlignment="Right" Margin="10 0" Background="#8BC34A" Foreground="#fff" Padding="15 5" Command="{Binding ClearLocationListViewCommand}" Visibility="{Binding IsVisibleLocationList}"/>
                <Button Content="Annuller" Grid.Row="4" HorizontalAlignment="Right" Margin="10 0" Background="#8BC34A" Foreground="#fff" Padding="15 5" Command="{Binding ClearProductListViewCommand}" Visibility="{Binding IsVisibleProductList}"/>

            </Grid>
        </Grid>

        <Grid Height="50"
              Grid.Row="0"
              Background="#455A64">
            <StackPanel Orientation="Horizontal"
                        VerticalAlignment="Top"
                        Height="50"
                        Background="#455A64">

                <Button HorizontalAlignment="Stretch"
                        Style="{StaticResource CustomButtonStyle}"
                        Padding="15 5">
                    <interactivity:Interaction.Behaviors>
                        <core:EventTriggerBehavior EventName="Click">
                            <core:NavigateToPageAction TargetPage="OsOs.View.ForsideView" />
                        </core:EventTriggerBehavior>
                    </interactivity:Interaction.Behaviors>
                    <Image Source="../../Assets/ViewImg/OsOs_white.png"
                           Height="30" />
                </Button>
                <Button Content="Lager"
                        Flyout="{StaticResource DeclarativeAttachedFlyout_Lager}"
                        Style="{StaticResource CustomButtonStyle}"
                        Visibility="{Binding LagerSmallView}" />
                <Button Content="Ordre"
                        Flyout="{StaticResource DeclarativeAttachedFlyout_Ordre}"
                        Style="{StaticResource CustomButtonStyle}"
                        Visibility="{Binding OrderSmallView}" />
                <Button Content="Rapporter"
                        Flyout="{StaticResource DeclarativeAttachedFlyout_Rapport}"
                        Style="{StaticResource CustomButtonStyle}"
                        Visibility="{Binding ReportSmallView}" />
                <Button Content="Grunddata"
                        Flyout="{StaticResource DeclarativeAttachedFlyout_Grunddata}"
                        Style="{StaticResource CustomButtonStyle}"
                        Visibility="{Binding GrundSmallView}" />

            </StackPanel>
            <Button HorizontalAlignment="Right"
                    Content="Logud"
                    Foreground="#fff"
                    Background="#8BC34A"
                    Padding="15 5"
                    Margin="0 0 15 0"
                    Command="{Binding}">
                <interactivity:Interaction.Behaviors>
                    <core:EventTriggerBehavior EventName="Click">
                        <core:NavigateToPageAction TargetPage="OsOs.MainPage" />
                    </core:EventTriggerBehavior>
                </interactivity:Interaction.Behaviors>
            </Button>
        </Grid>

        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="50"/>

            </Grid.ColumnDefinitions>

            <Grid.RowDefinitions>
                <RowDefinition Height="100"/>
                <RowDefinition Height="50"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="50"/>
            </Grid.RowDefinitions>
            <TextBlock Text="Grunddata" FontSize="40" Grid.Column="1" VerticalAlignment="Center"/>
            <Grid Grid.Column="1" Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Button Grid.Column="0" Content="Artikelregister" Style="{StaticResource PivotHeaderStyleInactive}">
                    <interactivity:Interaction.Behaviors>
                        <core:EventTriggerBehavior EventName="Click">
                            <core:NavigateToPageAction TargetPage="OsOs.View.Grunddata.ArtikelregisterView"/>
                        </core:EventTriggerBehavior>
                    </interactivity:Interaction.Behaviors>
                </Button>
                <Button Grid.Column="1" Content="Kunder" Style="{StaticResource PivotHeaderStyleInactive}">
                    <interactivity:Interaction.Behaviors>
                        <core:EventTriggerBehavior EventName="Click">
                            <core:NavigateToPageAction TargetPage="OsOs.View.Grunddata.KunderView"/>
                        </core:EventTriggerBehavior>
                    </interactivity:Interaction.Behaviors>
                </Button>
                <Button Grid.Column="2" Content="Lagerplads" Style="{StaticResource PivotHeaderStyleActive}">
                    <interactivity:Interaction.Behaviors>
                        <core:EventTriggerBehavior EventName="Click">
                            <core:NavigateToPageAction TargetPage="OsOs.View.Grunddata.LagerpladserView"/>
                        </core:EventTriggerBehavior>
                    </interactivity:Interaction.Behaviors>
                </Button>
                <Button Grid.Column="3" Content="Enheder" Style="{StaticResource PivotHeaderStyleInactive}">
                    <interactivity:Interaction.Behaviors>
                        <core:EventTriggerBehavior EventName="Click">
                            <core:NavigateToPageAction TargetPage="OsOs.View.Grunddata.EnhederView"/>
                        </core:EventTriggerBehavior>
                    </interactivity:Interaction.Behaviors>
                </Button>
                <Button Grid.Column="4" Content="Brugere" Style="{StaticResource PivotHeaderStyleInactive}">
                    <interactivity:Interaction.Behaviors>
                        <core:EventTriggerBehavior EventName="Click">
                            <core:NavigateToPageAction TargetPage="OsOs.View.Grunddata.BrugereView"/>
                        </core:EventTriggerBehavior>
                    </interactivity:Interaction.Behaviors>
                </Button>

            </Grid>
            <Grid BorderThickness="2" BorderBrush="#8BC34A" Grid.Column="1" Grid.Row="2" Background="#fff">
                <Grid VerticalAlignment="Stretch" HorizontalAlignment="Stretch">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="680"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="50"/>
                            <RowDefinition Height="50"/>
                            <RowDefinition Height="50"/>
                            <RowDefinition Height="25"/>
                            <RowDefinition Height="*"/>
                            <!--                            <RowDefinition Height="50"/>-->
                            <!--                            <RowDefinition Height="50"/>-->
                        </Grid.RowDefinitions>
                        <TextBlock Text="Lagerplads" VerticalAlignment="Center" Margin="20 0"/>
                        <TextBox Grid.Column="1" Text="{Binding LocationId, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Row="0" MaxLength="50"  VerticalAlignment="Center" HorizontalAlignment="Left" Width="300"/>
                        <Rectangle Grid.Column="1" Grid.Row="0" VerticalAlignment="Center" Height="32" Fill="#888" Opacity="0.2" Width="300" HorizontalAlignment="Left"/>

                        <ToggleButton IsChecked="{Binding ToggleVisibilityLocationList, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  Grid.Column="1" Grid.Row="0" Margin="310 0 0 0" Content="Søg Lagerplads" IsThreeState="False"/>
                        <TextBlock Text="Reserveret" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="20 0" Grid.Column="0" Grid.Row="1"/>
                        <CheckBox Grid.Column="1" Grid.Row="1" IsThreeState="False" IsChecked="{Binding ToggleVisibilitySearchProduct, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                        <TextBox Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" Text="{Binding ProductName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Visibility="{Binding IsVisibleSearchProduct}" HorizontalAlignment="Left" Margin="100 0 0 0" Width="200"/>
                        <Rectangle Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" Height="32" Fill="#888" Opacity="0.2" Width="200" HorizontalAlignment="Left" Margin="100 0 0 0" Visibility="{Binding IsVisibleSearchProduct}"/>

                        <ToggleButton Visibility="{Binding IsVisibleSearchProduct, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" IsChecked="{Binding ToggleVisibilityProductList, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  Grid.Column="1" Grid.Row="1" Margin="310 0 0 0" Content="Søg Artikel" IsThreeState="False"/>

                        <Button Content="Ok" Grid.Column="0" Grid.Row="2" Command="{Binding SubmitLocation}" VerticalAlignment="Center" HorizontalAlignment="Stretch" Margin="20 0" Background="#8BC34A" Foreground="#fff"/>
                        <Button Content="Slet" Grid.Column="1" Grid.Row="2" Command="{Binding OneDeleteButtonToRuleThemAllCommand}" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="20 0" Width="160" Background="#8BC34A" Foreground="#fff"/>

                        <Rectangle HorizontalAlignment="Stretch" Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="2" Fill="#8BC34A" Height="5" Margin="10 00"/>
                        <Grid Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="50"/>

                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Text="Lagerpladsgenerering:" FontSize="20" VerticalAlignment="Center" Margin="20 0"/>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="150"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <!--                                    <ColumnDefinition Width="*"/>-->
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Række:" Grid.Column="0" Margin="20 0" VerticalAlignment="Top"/>
                                <TextBlock Text="Stabel:" Grid.Column="1"/>
                                <TextBlock Text="Højde:" Grid.Column="2"/>
                                <TextBox Grid.Column="0" VerticalAlignment="Top" MaxLength="2" Margin="20 40 40 0" Text="{Binding Row, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"/>
                                <Grid Grid.Column="1" Margin="40 0 40 0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="40"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBox Grid.Column="0" Margin="0 40 0 0" VerticalAlignment="Top" MaxLength="2" Text="{Binding StartSpace, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                                    <TextBlock Text="-" Grid.Column="1" VerticalAlignment="Top" Margin="0 30 0 0" FontSize="30" HorizontalAlignment="Center"/>
                                    <TextBox Grid.Column="2" Margin="0 40 0 0" VerticalAlignment="Top" MaxLength="2" Text="{Binding EndSpace, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                                </Grid>
                                <Grid Grid.Column="2" Margin="40 0 40 0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="40"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBox Grid.Column="0" Margin="0 40 0 0" VerticalAlignment="Top" Text="{Binding StartHeight, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" MaxLength="1"/>
                                    <TextBlock Text="-" Grid.Column="1" VerticalAlignment="Top" Margin="0 30 0 0" FontSize="30" HorizontalAlignment="Center"/>
                                    <TextBox Grid.Column="2" Margin="0 40 0 0" VerticalAlignment="Top" Text="{Binding EndHeight, UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" MaxLength="1"/>

                                </Grid>

                            </Grid>
                            <Button Content="Ok" Grid.Column="0" Command="{Binding SubmitGenerateLocations}" Width="160" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="20 0" Background="#8BC34A" Foreground="#fff"/>


                        </Grid>
                    </Grid>
                </Grid>
            </Grid>

        </Grid>
    </Grid>
</Page>