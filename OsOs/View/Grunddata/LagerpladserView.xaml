        <DataTemplate x:Key="LocationListViewItemTemplate">
            <Grid>
                <StackPanel HorizontalAlignment="Stretch" Orientation="Horizontal">
                    <TextBlock Text="{Binding Location}" Width="100"></TextBlock>
                    <CheckBox IsThreeState="False" IsChecked="{Binding Reserved}" IsDoubleTapEnabled="False" IsHoldingEnabled="False" IsRightTapEnabled="False" IsTapEnabled="False" ManipulationMode="None"></CheckBox>
                    <Rectangle Opacity="0.0" VerticalAlignment="Stretch" Width="130" Fill="#fff" Canvas.ZIndex="1200" Margin="-130 0 0 0"></Rectangle>
                    <TextBlock Text="{Binding Product.Name}" HorizontalAlignment="Left" Width="150" Opacity="0.4"/>
                    <TextBlock HorizontalAlignment="Left" MaxWidth="80" Opacity="0.4">
                        <Run Text="{Binding Quantity}"/>
                        <Run Text="Stk"></Run>
                    </TextBlock>
                </StackPanel>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ProductListViewItemTemplate">
            <Grid>
                <StackPanel HorizontalAlignment="Stretch" Orientation="Horizontal">
                    <TextBlock Text="{Binding Name}" Width="200"/>
                    <TextBlock Text="{Binding Description}"/>
